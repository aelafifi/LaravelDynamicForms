{
  "name": "elmag/laravel-dynamic-forms",
  "description": "A Laravel package for dynamic forms management",
  "keywords": [
    "laravel",
    "dynamic",
    "form"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmed S. El-Afifi",
